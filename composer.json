{
    "name": "jolicode/seo-override",
    "description": "Override your seo markup on the fly",
    "keywords": ["seo"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lo√Øck Piera",
            "email": "pyrech@gmail.com"
        },
        {
            "name": "Damien Alexandre",
            "email": "dalexandre@jolicode.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Joli\\SeoOverride\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joli\\SeoOverride\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "doctrine/doctrine-bundle": "^1.12|~2.0",
        "doctrine/orm": "^2.7",
        "friendsofphp/php-cs-fixer": "^2.2",
        "phpdocumentor/reflection-docblock": "^4.0|~5.0",
        "phpspec/prophecy": "^1.7",
        "phpspec/prophecy-phpunit": "^2.0",
        "symfony/framework-bundle": "^3.4.25|~4.0|~5.0",
        "symfony/mime": "^4.4.|~5.0",
        "symfony/phpunit-bridge": "~5.0",
        "symfony/twig-bundle": "^3.4|~4.0|~5.0",
        "symfony/yaml": "^3.4|~4.0|~5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<1.12",
        "doctrine/common": "<2.8",
        "doctrine/orm": "<2.7",
        "doctrine/persistence": "<1.3.8",
        "phpspec/prophecy": "<1.10.0",
        "sebastian/comparator": "<1.2.3",
        "symfony/doctrine-bridge": "<3.4",
        "symfony/framework-bundle": "<3.4.25",
        "symfony/twig-bundle": "<3.4",
        "symfony/yaml": "<3.4",
        "symfony/symfony": "<3.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
